<main class="row contain">
  {{collection-meta-panel model=model.collection datasets=model.datasets}}

  <div class="content right">
    <div class="col m11 offset-m1">
      {{#each model.datasets as |dataset|}}
        {{dataset-card dataset=dataset action="toggleModal"}}
      {{/each}}
    </div>
  </div>
</main>
