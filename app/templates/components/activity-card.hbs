{{activity-card-header group=group count=model.length isProfilePage=isProfilePage}}
<div class="{{if isCollapsed 'c-activity-card-body-collapsed'}}">
  {{#each model as |dataset|}}
    {{activity-card-item dataset=dataset}}
  {{else}}
    <div class="flex justify-center items-center u-full-height">
      <p class="p3 ta-center">{{emptyText}}</p>
    </div>
  {{/each}}
</div>
