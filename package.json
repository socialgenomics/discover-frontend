{
  "name": "repositive.io",
  "version": "0.0.0",
  "description": "Frontend app for repositive.io",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server --proxy http://testing.api.repositive.io",
    "build": "ember build",
    "test": "ember test",
    "deployLive": "ssh -f -L 6379:discover.repositive.io:6379 discover.repositive.io -N;  echo 'Please enter your AWS Access Key ID: '; read AWS_KEY_ID; export AWS_KEY_ID; echo 'Please enter your AWS Secret Access Key: '; read AWS_ACCESS_KEY; export AWS_ACCESS_KEY; ember deploy --environment=production",
    "deployTesting": "ssh -f -L 6379:testing.discover.repositive.io:6379 testing.discover.repositive.io -N;  echo 'Please enter your AWS Access Key ID: '; read AWS_KEY_ID; export AWS_KEY_ID; echo 'Please enter your AWS Secret Access Key: '; read AWS_ACCESS_KEY; export AWS_ACCESS_KEY; ember deploy --environment=testing",
    "containerized": "ember server --proxy http://$(boot2docker ip)"
  },
  "repository": "https://bitbucket.org/repositive/repositive.io",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "repositive.io",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.0",
    "ember-browserify": "ef4/ember-browserify",
    "ember-cli": "^0.2.1",
    "ember-cli-app-version": "0.3.2",
    "ember-cli-babel": "^4.0.0",
    "ember-cli-content-security-policy": "0.3.0",
    "ember-cli-dependency-checker": "0.0.8",
    "ember-cli-deploy": "^0.4.1",
    "ember-cli-htmlbars": "0.7.4",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-materialize": "^0.15.3",
    "ember-cli-qunit": "0.3.9",
    "ember-cli-sass": "^3.3.1",
    "ember-cli-simple-auth": "0.7.3",
    "ember-cli-simple-auth-cookie-store": "0.7.3",
    "ember-cli-simple-auth-oauth2": "0.7.3",
    "ember-cli-uglify": "1.0.1",
    "ember-data": "1.0.0-beta.15",
    "ember-deploy-redis": "0.0.2",
    "ember-deploy-s3": "0.0.3",
    "ember-export-application-global": "^1.0.2",
    "ember-flash-messages": "0.5.2",
    "ember-key-responder": "^0.2.1",
    "ember-modal-dialog": "^0.7.0",
    "ember-moment": "1.1.1",
    "ember-new-computed": "^1.0.0",
    "ember-radio-button": "^1.0.4",
    "ember-truth-helpers": "0.0.5",
    "ember-validations": "2.0.0-alpha.3",
    "moment": "^2.10.2",
    "torii": "^0.5.1",
    "underscore": "^1.8.2"
  }
}
