    "ember-cli-release": "0.2.3",
    "ember-cli-sass": "^4.0.1",
    "ember-cli-simple-auth": "0.8.0",
    "ember-cli-sri": "^1.0.1",
    "ember-cli-uglify": "^1.0.1",
    "ember-data": "1.13.7",
    "ember-deploy-redis": "0.0.7",
