    "ember-cli-babel": "^5.0.0",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.0.0",
    "ember-cli-deploy": "^0.4.3",
    "ember-cli-flash": "1.3.3",
    "ember-cli-htmlbars": "0.7.9",
